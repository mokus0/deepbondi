#!/bin/sh
# C header template

hFile=`basename "$1"`
defname=`echo -n "$hFile" | sed 's/[^A-Za-z0-9]/_/g'`

fullName=`fullName`
fullName=`echo -n $fullName`

cat > "$1" <<EOF
	/*
	 *  <$hFile>
	 *    Copyright `date +%Y` $fullName - All Rights Reserved.
	 */

#ifndef __${defname}__
#define __${defname}__





#endif /* __${defname}__ */
